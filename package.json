{
  "name": "jupyter-jacdac",
  "version": "1.1.4",
  "description": "Connect to Jacdac devices from JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "Jacdac",
    "embedded",
    "TinyML",
    "TensorFlow",
    "TensorFlow Lite"
  ],
  "homepage": "https://github.com/microsoft/jupyter-jacdac",
  "bugs": {
    "url": "https://github.com/microsoft/jupyter-jacdac/issues"
  },
  "license": "MIT",
  "author": "Microsoft Corporation",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/jupyter-jacdac.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "doc",
              "release": "patch"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "patch",
              "release": "patch"
            },
            {
              "type": "minor",
              "release": "minor"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "feature",
              "release": "minor"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "yarn.lock"
          ]
        }
      ],
      [
        "@semantic-release/exec",
        {
          "successCmd": "curl https://mybinder.org/v2/gh/microsoft/jupyter-jacdac/main?urlpath=lab"
        }
      ]
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.2",
    "@jupyterlab/apputils": "^2.0.2",
    "@jupyterlab/launcher": "^2.0.2",
    "@jupyterlab/mainmenu": "^2.0.2",
    "@jupyterlab/notebook": "^2.0.2",
    "@lumino/widgets": "^1.14.0",
    "@semantic-release/exec": "^5.0.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "rimraf": "^2.6.1",
    "semantic-release": "^19.0.3",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
